{
    "readme": "基础（柱状图bar、折线图line、饼图pie、散点图scatter、漏斗图funnel）图表",
    "title": [
        {
            "show":"",
            "text": "",
            "subtext": "",
            "left": "center",
            "top": "top",
            "textStyle": {
                "color": "#ffffff",
                "fontSize": 20,
                "fontFamily": "PingfangRegular"
            },
            "subtextStyle": {
                "color": "#ffffff",
                "fontSize": 20,
                "fontFamily": "PingfangRegular"
            },
            "itemGap":1
        }
    ],
    "tooltip": {
        "trigger": "axis",
        "backgroundColor": "rgba(0, 0, 0, 0.5)",
        "borderColor": "#ffffff",
        "padding": [
            5,
            10
        ],
        "textStyle": {
            "color": "#ffffff",
            "fontFamily": "PingfangRegular",
            "fontSize": 20
        },
        "axisPointer": {
            "type": "cross",
            "label": {
                "backgroundColor": "transparent",
                "borderColor": "#aaa",
                "borderWidth": 0,
                "color": "#01eef3",
                "fontSize": 20,
                "fontFamily": "PingfangRegular"
            },
            "lineStyle": {
                "type": "dashed",
                "color": "#999",
                "width": 1,
                "opacity": 0.5
            }
        },
        "formatter": "{b}<br/>{a0}：{c0}"
    },
    "legend": [
        {
            "show": true,
            "orient": "horizontal",
            "data": [],
            "left": "right",
            "top": "top",
            "align": "left",
            "textStyle": {
                "color": "#ffffff",
                "fontSize": 20,
                "fontFamily": "PingfangRegular"
            }
        }
    ],
    "grid": [
        {
            "show": false,
            "top": 60,
            "bottom": 60,
            "right": 30,
            "left": 80,
            "borderWidth":0
        }
    ],
    "xAxis": [
        {
            "type": "category",
            "data": [],
            "boundaryGap": true,
            "nameTextStyle":{
              "color": "#01eef3",
              "fontSize": 20,
              "fontFamily": "PingfangRegular"
            },
            "splitLine": {
                "lineStyle": {
                    "type": "dashed",
                    "color": "#1a2933"
                },
                "show": false
            },
            "axisLine": {
                "show":true,
                "lineStyle": {
                    "color": "#01eef3",
                    "width": 1
                }
            },
            "axisLabel": {
                "show": true,
                "textStyle": {
                    "color": "#01eef3",
                    "fontSize": 20,
                    "fontFamily": "PingfangRegular"
                }
            }
        }
    ],
    "yAxis": [
        {
            "type": "value",
            "nameTextStyle":{
              "color": "#01eef3",
              "fontSize": 20,
              "fontFamily": "PingfangRegular"
            },
            "splitLine": {
                "lineStyle": {
                    "type": "dashed",
                    "color": "#1a2933"
                },
                "show": true
            },
            "axisLine": {
                "show":false,
                "lineStyle": {
                    "color": "#01eef3",
                    "width": 1
                }
            },
            "axisLabel": {
                "show": true,
                "textStyle": {
                    "color": "#01eef3",
                    "fontSize": 20,
                    "fontFamily": "PingfangRegular"
                }
            }
        }
    ],
    "series": [
        {
            "name": "",
            "data": [],
            "type": "",
            "label": {
                "show": true,
                "position": "top",
                "textStyle": {
                    "color": "#ffffff",
                    "fontSize": 20,
                    "fontFamily": "PingfangRegular"
                }
            },            
            "itemStyle": {
                "borderRadius": 5,
                "borderColor": "#001e4a",
                "borderWidth": 0
            },
            "labelLine": {
                "show": false,
                "length": 100,
                "length2": 100,
                "smooth": 0.2,
                "lineStyle": {
                    "width": 3
                }
            },
            
            "padAngle": 1.4,
            "radius": ["40%","70%"],
            "avoidLabelOverlap": true,

            "smooth": true,
            "areaStyle": {
                "color":""
            },
            "symbol":"none",
            "symbolSize": 20,

            "barWidth": 40
        }
